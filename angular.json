{
  "build": {
    "executor": "@nrwl/angular:webpack-browser",
    "options": {
      "customWebpackConfig": {
        "path": "apps/api/webpack.config.js"
      }
    }
  },
  "serve": {
    "executor": "@nrwl/angular:webpack-server"
  },
  "$schema": "./node_modules/nx/schemas/workspace-schema.json",
  "version": 2,
  "styles": [
    "/node_modules/primeng/resources/themes/lara-light-blue/theme.css",
    "/distnode_modules/primeng/resources/primeng.min.css",
    "/node_modules/primeicons/primeicons.css"
  ],
  "projects": {
    "api": "apps/api",
    "api-interfaces": "libs/api-interfaces",
    "project-ecommerce": "apps/project-ecommerce",
    "project-ecommerce-e2e": "apps/project-ecommerce-e2e"
  }
}
